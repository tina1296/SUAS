<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/muscle1" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_below="@id/imageView3"
        android:text="나 자신과 타협하지 마세요." />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_below="@id/textView8"
        android:layout_centerInParent="true"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/diet2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_below="@id/imageView5"
        android:text="생각이 바뀌면 몸도 변하기 시작한다." />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_below="@id/textView9"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/muscle2" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_below="@id/imageView6"
        android:text="몸매는 헬스장에서 관리하고, 다이어트는 부엌에서 해라. " />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_below="@id/textView10"
        android:layout_centerInParent="true"
        app:srcCompat="@drawable/diet1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_below="@id/imageView2"
        android:layout_centerHorizontal="true"
        android:text="하얀음식은 먹지 않아요. 독이기 때문이죠-미란다커" />

</RelativeLayout>